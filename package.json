{
  "name": "@doitintl/doit-mcp-server",
  "version": "0.1.35",
  "description": "DoiT official MCP Server",
  "keywords": [
    "doit",
    "mcp",
    "mcp-server"
  ],
  "license": "MIT",
  "author": "",
  "type": "module",
  "bin": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "compile": "tsc",
    "build": "tsc && shx chmod +x dist/*.js",
    "watch": "tsc --watch",
    "test": "vitest run",
    "deploy": "yarn build && npm publish --access public",
    "type-check": "tsc --noEmit",
    "lint": "biome lint src/",
    "lint:fix": "biome lint --write src/",
    "format": "biome format src/",
    "format:fix": "biome format --write src/",
    "check": "biome check src/",
    "check:fix": "biome check --write src/"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.3",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.4.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.1",
    "@types/node": "^22",
    "shx": "^0.3.4",
    "typescript": "^5.7.2",
    "vitest": "^3.1.3"
  },
  "engines": {
    "node": ">=18"
  }
}
